{
    "name": "@cordy/electro-cli",
    "version": "1.1.3",
    "description": "CLI for @cordy/electro â€” dev server, build, and code generation commands",
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "keywords": [
        "electron",
        "cli",
        "vite",
        "dev-server",
        "build-tool"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cordy-dev/electro.git",
        "directory": "packages/electro-cli"
    },
    "bugs": {
        "url": "https://github.com/cordy-dev/electro/issues"
    },
    "homepage": "https://github.com/cordy-dev/electro#readme",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "bun": ">=1.0.0"
    },
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "files": [
        "dist"
    ],
    "bin": {
        "electro": "./dist/index.mjs"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs"
        }
    },
    "scripts": {
        "build": "tsdown",
        "electro": "bun src/index.ts",
        "prepublishOnly": "bun run build"
    },
    "peerDependencies": {
        "@cordy/electro": "1.1.2",
        "electron": ">=40.4.1",
        "vite": ">=8.0.0"
    },
    "dependencies": {
        "@cordy/electro-generator": "1.1.2"
    },
    "devDependencies": {
        "@cordy/electro": "workspace:*",
        "@types/node": "^25.2.3",
        "cac": "^6.7.14",
        "electron": "^40.4.1",
        "magic-string": "^0.30.21",
        "tsdown": "^0.20.3",
        "vite": "^8.0.0-beta.14"
    }
}
