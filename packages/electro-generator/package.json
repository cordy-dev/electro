{
    "name": "@cordy/electro-generator",
    "version": "1.1.2",
    "description": "Code generator for @cordy/electro â€” scans features and emits preload scripts and bridge types",
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "keywords": [
        "electron",
        "codegen",
        "preload",
        "bridge",
        "typescript"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cordy-dev/electro.git",
        "directory": "packages/electro-generator"
    },
    "bugs": {
        "url": "https://github.com/cordy-dev/electro/issues"
    },
    "homepage": "https://github.com/cordy-dev/electro#readme",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "bun": ">=1.0.0"
    },
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs"
        }
    },
    "scripts": {
        "build": "tsdown",
        "test": "vitest",
        "prepublishOnly": "bun run build"
    },
    "peerDependencies": {
        "@cordy/electro": "1.1.2"
    },
    "dependencies": {
        "oxc-parser": "^0.114.0",
        "tinyglobby": "^0.2.15"
    },
    "devDependencies": {
        "@cordy/electro": "workspace:*",
        "tsdown": "^0.20.3",
        "vitest": "v4.1.0-beta.3"
    }
}
